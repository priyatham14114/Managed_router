<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:form="sap.ui.layout.form"
>
    <form:SimpleForm
        id="idpersonalInfoForm"
        editable="false"
        layout="ColumnLayout"
        columnsM="2"
        columnsL="3"
        columnsXL="4"
        binding="{personalInfo}"
    >
        <Label
            id="idphone_numberLabel"
            text="Personal Number"
        />
        <Text
            id="idphone_numberText"
            text="{phone_number}"
        />

        <Label
            id="idemailLabel"
            text="Email"
        />
        <Text
            id="idemailText"
            text="{email}"
        />
        <Label
            id="idbiographyLabel"
            text="Biography"
        />
        <Text
            id="idbiographyText"
            text="{biography}"
        />
        
    </form:SimpleForm>
</core:FragmentDefinition>
